import random
import numpy as np

def testdata():
  #this is the preferences of people 1-6 (p1-p6)

  p1 =  [random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10)]
  p2 = [random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10)]
  p3 =  [random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10)]
  p4 = [random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10)]
  p5 = [random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10)]
  p6 = [random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10),random.randint(1,10)]

  #appends these matrices to make one large matrix with all the people
  costmatrix = [p1,p2,p3,p4,p5,p6]



  #calculates the number of assignments per hospital based on givens
  numberofhospitals = 5
  numberofpeople = 6
  numberofassignments = int(np.ceil(numberofpeople/numberofhospitals))
  print(numberofassignments)

  fullmatrix = np.reshape(costmatrix, [numberofpeople,numberofhospitals])

  return fullmatrix #full matrix can be used as an input into hungarian algorithm code
